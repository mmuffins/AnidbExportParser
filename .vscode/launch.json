{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ParserConsole (run)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "prelaunch: cleanup â†’ restore",
      "program": "dotnet",
      "args": [
        "${workspaceFolder}/ParserConsole/bin/${config:dotnet.defaults.buildConfiguration}/${config:dotnet.defaults.framework}/ParserConsole.dll",
        "convert",
        "--input", "${config:debug.inputFile}",
        "--output", "${config:debug.outputFile}"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "justMyCode": true,
      "requireExactSource": false
    },
  ]
}